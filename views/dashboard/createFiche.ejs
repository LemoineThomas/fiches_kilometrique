<%- include ('../layout.ejs') %>
<form>
    <div class="form-group">
        <label for="exampleFormControlSelect1">Entité</label>
        <select class="form-control" id="exampleFormControlSelect1">
          <option>liste d'entités</option>
        </select>
    </div>
    <div class="form-group">
        <label for="exampleFormControlSelect2">Véhicule</label>
        <select class="form-control" id="exampleFormControlSelect2">
          <option>liste des véhicules</option>
        </select>
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput3">Date</label>
        <input type="date" class="form-control" id="formGroupExampleInput3" placeholder="Ex: 2CV">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput4">Lieu de départ</label>
        <input type="text" class="form-control" id="formGroupExampleInput4" placeholder="Ex: 2003">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput5">Lieu d'arrivée</label>
        <input type="text" class="form-control" id="formGroupExampleInput5" placeholder="Ex: 158-MQL-974">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput6">Object du déplacement</label>
        <input type="text" class="form-control" id="formGroupExampleInput6" placeholder="Ex: 158-MQL-974">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput7">Commentaire</label>
        <input type="text" class="form-control" id="formGroupExampleInput7" placeholder="Ex: 158-MQL-974">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput8">Relevé du compteur au départ</label>
        <input type="number" class="form-control" id="formGroupExampleInput8" placeholder="Ex: 158-MQL-974" min="0" max="1000000">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput9">Relevé du compteur à l'arrivée</label>
        <input type="number" class="form-control" id="formGroupExampleInput8" placeholder="Ex: 158-MQL-974" min="0" max="1000000">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput10">Distance parcourus</label>
        <input type="text" class="form-control" id="formGroupExampleInput8" placeholder="Ex: 158-MQL-974">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput11">Total dekilomètre parcourus</label>
        <input type="text" class="form-control" id="formGroupExampleInput8" placeholder="Ex: 158-MQL-974">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput12">Montant de l'indemnité</label>
        <input type="text" class="form-control" id="formGroupExampleInput8" placeholder="Ex: 158-MQL-974">
    </div>
    
    <button type="submit" class="btn btn-primary">Créer</button>
</form>
<%- include ('../footer.ejs') %>