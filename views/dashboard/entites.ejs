<%- include ('../layout.ejs') %>
<h2 class="mt-4"><%= title %></h2>

<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Nom</th>
        <th scope="col">Type</th>
      </tr>
    </thead>
    <tbody>
        <%  entites.forEach(element => { %>
            <tr>
                <td><%= element.nom %></td>
                <td><%= element.type %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<%- include ('../footer.ejs') %>